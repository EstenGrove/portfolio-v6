{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"outDir": "./dist",
		"rootDir": "./src",
		"strict": true,
		"allowJs": false,
		"skipLibCheck": true,
		"allowUnreachableCode": false,
		"alwaysStrict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"useUnknownInCatchVariables": true,
		"types": ["node"],
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx"
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules", "dist"]
}
